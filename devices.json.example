{
  "devices": [
    {
      "id": "cisco-sw-core",
      "name": "Cisco Core Switch",
      "type": "cisco",
      "transport": ["ssh"],
      "host": "192.168.1.1",
      "port": 22,
      "username": "admin",
      "password": "${CISCO_CORE_PASSWORD}",
      "enablePassword": "${CISCO_CORE_ENABLE}",
      "vpn": "openvpn",
      "tags": ["core", "network", "switch"]
    },
    {
      "id": "hp-sw-floor2",
      "name": "HP Switch Floor 2",
      "type": "hp",
      "transport": ["ssh"],
      "host": "192.168.1.2",
      "port": 22,
      "username": "manager",
      "password": "${HP_FLOOR2_PASSWORD}",
      "vpn": "wireguard",
      "tags": ["access", "network", "switch"]
    },
    {
      "id": "fg-01",
      "name": "Fortigate Firewall",
      "type": "fortigate",
      "transport": ["rest", "ssh"],
      "host": "192.168.1.254",
      "apiUrl": "https://192.168.1.254",
      "apiKey": "${FORTIGATE_API_KEY}",
      "username": "admin",
      "password": "${FORTIGATE_SSH_PASSWORD}",
      "tags": ["firewall", "network", "perimeter"]
    },
    {
      "id": "mikrotik-gw",
      "name": "Mikrotik Gateway Router",
      "type": "mikrotik",
      "transport": ["rest", "ssh"],
      "host": "192.168.1.253",
      "apiUrl": "https://192.168.1.253",
      "port": 22,
      "username": "admin",
      "password": "${MIKROTIK_PASSWORD}",
      "extra": { "apiPort": 8728 },
      "tags": ["router", "network", "gateway"]
    },
    {
      "id": "tb-server",
      "name": "ThingsBoard Server",
      "type": "thingsboard",
      "transport": ["rest"],
      "apiUrl": "http://192.168.1.100:8080",
      "username": "tenant@thingsboard.org",
      "password": "${THINGSBOARD_PASSWORD}",
      "tags": ["iot", "platform"]
    },
    {
      "id": "esphome-01",
      "name": "ESPHome Controller",
      "type": "esphome",
      "transport": ["rest"],
      "apiUrl": "http://192.168.1.50:6052",
      "apiKey": "${ESPHOME_API_KEY}",
      "vpn": "zerotier",
      "tags": ["iot", "esphome"]
    },
    {
      "id": "espconnect-01",
      "name": "ESPConnect Hub",
      "type": "espconnect",
      "transport": ["rest"],
      "apiUrl": "http://192.168.1.51:80",
      "apiKey": "${ESPCONNECT_API_KEY}",
      "tags": ["iot", "espconnect"]
    },
    {
      "id": "tuya-cloud",
      "name": "Tuya Cloud Account",
      "type": "tuya",
      "transport": ["rest"],
      "apiUrl": "https://openapi.tuyaus.com",
      "extra": {
        "clientId": "${TUYA_CLIENT_ID}",
        "clientSecret": "${TUYA_CLIENT_SECRET}"
      },
      "tags": ["iot", "cloud", "tuya"]
    },
    {
      "id": "sonoff-cloud",
      "name": "Sonoff/eWeLink Cloud",
      "type": "sonoff",
      "transport": ["rest"],
      "apiUrl": "https://us-apia.coolkit.cc",
      "extra": {
        "appId": "${EWELINK_APP_ID}",
        "appSecret": "${EWELINK_APP_SECRET}"
      },
      "tags": ["iot", "cloud", "sonoff"]
    },
    {
      "id": "qnap-nas",
      "name": "QNAP NAS",
      "type": "qnap",
      "transport": ["rest"],
      "apiUrl": "https://192.168.1.200:8443",
      "username": "admin",
      "password": "${QNAP_PASSWORD}",
      "tags": ["storage", "nas"]
    },
    {
      "id": "synology-nas",
      "name": "Synology NAS",
      "type": "synology",
      "transport": ["rest"],
      "apiUrl": "https://192.168.1.201:5001",
      "username": "admin",
      "password": "${SYNOLOGY_PASSWORD}",
      "tags": ["storage", "nas"]
    },
    {
      "id": "proxmox-01",
      "name": "Proxmox Server",
      "type": "proxmox",
      "transport": ["rest"],
      "apiUrl": "https://192.168.1.10:8006",
      "username": "root@pam",
      "password": "${PROXMOX_PASSWORD}",
      "tags": ["hypervisor", "server"]
    },
    {
      "id": "esxi-01",
      "name": "ESXi Server",
      "type": "esxi",
      "transport": ["rest"],
      "host": "192.168.1.11",
      "username": "root",
      "password": "${ESXI_PASSWORD}",
      "tags": ["hypervisor", "server"]
    },
    {
      "id": "dahua-nvr-01",
      "name": "Dahua NVR",
      "type": "dahua-nvr",
      "transport": ["rest"],
      "apiUrl": "http://192.168.1.108",
      "username": "admin",
      "password": "${DAHUA_NVR_PASSWORD}",
      "tags": ["cctv", "nvr", "surveillance"]
    },
    {
      "id": "dahua-dss-01",
      "name": "Dahua DSS Pro",
      "type": "dahua-dss",
      "transport": ["rest"],
      "apiUrl": "https://192.168.1.109:443",
      "username": "system",
      "password": "${DAHUA_DSS_PASSWORD}",
      "tags": ["cctv", "vms", "surveillance"]
    },
    {
      "id": "openstack-01",
      "name": "OpenStack Cloud",
      "type": "openstack",
      "transport": ["rest"],
      "apiUrl": "http://192.168.1.20:5000",
      "username": "admin",
      "password": "${OPENSTACK_PASSWORD}",
      "extra": {
        "project": "admin",
        "domain": "Default"
      },
      "tags": ["cloud", "hypervisor", "openstack"]
    },
    {
      "id": "hf-serial-01",
      "name": "HF5111 Serial Server",
      "type": "hiflying",
      "transport": ["rest"],
      "host": "192.168.1.50",
      "apiUrl": "http://192.168.1.50",
      "username": "admin",
      "password": "${HF_PASSWORD}",
      "extra": {
        "atPort": 49000,
        "serialPort": 8899
      },
      "tags": ["iot", "serial", "gateway"]
    }
  ]
}
